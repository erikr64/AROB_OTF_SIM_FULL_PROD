SELECT 
    completed_time,
    CASE WHEN completed_time IS NULL THEN DATEDIFF(seconds, query_start_time, CURRENT_TIMESTAMP) ELSE DATEDIFF(seconds, query_start_time, completed_time) END AS duration,
    state,
    * 
FROM TABLE(
    INFORMATION_SCHEMA.TASK_HISTORY(SCHEDULED_TIME_RANGE_START => TO_TIMESTAMP_LTZ(DATEADD(DAY, -7, CURRENT_TIMESTAMP)), SCHEDULED_TIME_RANGE_END   => TO_TIMESTAMP_LTZ(CURRENT_TIMESTAMP),RESULT_LIMIT => 1000))
WHERE schema_name = 'DBO' AND query_start_time IS NOT NULL
--AND
ORDER BY query_start_time DESC;

-- SELECT SYSTEM$CANCEL_QUERY('01bb4a07-030c-d176-0009-79d306acc1ee');

execute task Step_OA_RFPGen_Simulation
execute task AOG_Orch_Simulation



Select * from SPREETAIL_WAREHOUSE.INVENTORY.OOS_REALLOCATION

select * from INVENTORYSANDBOX.DBO.RFP_CURRENT_INVENTORYMODELCOMBINED_HISTORY

